{
  "name": "obsidian-rack-visualization",
  "version": "1.0.0",
  "description": "Generate server rack diagrams from RackML XML or text markup in code blocks",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.js",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.js production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "rack",
    "visualization",
    "diagram",
    "server"
  ],
  "author": "Harald Lapp",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
